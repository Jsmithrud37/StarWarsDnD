{
	"name": "datapad",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"build": "run-p build:*",
		"build-clean": "run-s clean build",
		"build:backend": "netlify-lambda build -c ./webpack.backend.js ./backend",
		"build:frontend": "react-scripts build",
		"check-peer-dependencies": "check-peer-deps",
		"clean": "run-p clean:*",
		"clean:backend": "del-cli backend-build/*",
		"clean:frontend": "del-cli build/*",
		"clean:test-reports": "del-cli reports/*",
		"eject": "react-scripts eject",
		"fix": "run-s fix:*",
		"fix:ecmascript": "eslint ./{backend,src}/**/*.{js,jsx,ts,tsx} --fix",
		"fix:package-json": "sort-package-json ./package.json",
		"fix:prettier": "prettier . --write",
		"postinstall": "npm run check-peer-dependencies",
		"lint": "run-p lint:*",
		"lint:ecmascript": "eslint ./{backend,src}/**/*.{js,jsx,ts,tsx}",
		"lint:package-json": "sort-package-json ./package.json --check",
		"lint:prettier": "prettier . --check",
		"lint:yaml": "eslint .github/workflows/**/*.{yml,yaml}",
		"start": "run-p start:*",
		"start:backend": "netlify-lambda serve -c ./webpack.backend.js ./backend",
		"start:frontend": "react-scripts start",
		"test": "react-scripts test --coverage --coverageDirectory=reports/coverage"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{js,jsx,ts,tsx}"
		]
	},
	"dependencies": {
		"@auth0/auth0-react": "^1.2.0",
		"@babel/runtime": "^7.12.5",
		"@material-ui/core": "^4.11.2",
		"@material-ui/icons": "^4.11.2",
		"@material-ui/lab": "^4.0.0-alpha.57",
		"@reduxjs/toolkit": "^1.5.0",
		"@widgetbot/react-embed": "1.2.2",
		"aws-lambda": "^1.0.6",
		"bootstrap": "^4.5.3",
		"fs-extra": "^9.0.1",
		"jquery": "^3.5.1",
		"mongodb": "^3.6.3",
		"mongodb-client-encryption": "^1.1.0",
		"mongoose": "^5.11.12",
		"popper.js": "^1.16.1",
		"react": "^17.0.1",
		"react-animated-burgers": "^1.2.7",
		"react-custom-scrollbars": "^4.2.1",
		"react-dom": "^17.0.1",
		"react-iframe": "1.8.0",
		"react-image": "^4.0.3",
		"react-list": "^0.8.16",
		"react-markdown": "^5.0.3",
		"react-redux": "^7.2.2",
		"react-scripts": "^4.0.1",
		"react-swipeable-views": "^0.13.9",
		"react-vertical-timeline-component": "^3.3.1",
		"redux": "^4.0.5",
		"saslprep": "1.0.1",
		"styled-components": "^5.2.1"
	},
	"devDependencies": {
		"@babel/core": "^7.12.10",
		"@babel/plugin-transform-object-assign": "^7.12.1",
		"@babel/preset-typescript": "^7.12.7",
		"@testing-library/dom": "^7.29.4",
		"@testing-library/jest-dom": "^5.11.9",
		"@testing-library/react": "^11.2.3",
		"@testing-library/user-event": "^12.6.0",
		"@types/aws-lambda": "^8.10.70",
		"@types/jest": "^26.0.20",
		"@types/mongoose": "^5.10.3",
		"@types/node": "^14.14.21",
		"@types/react": "^17.0.0",
		"@types/react-burger-menu": "^2.6.2",
		"@types/react-custom-scrollbars": "^4.0.7",
		"@types/react-dom": "^17.0.0",
		"@types/react-list": "^0.8.5",
		"@types/react-redux": "^7.1.15",
		"@types/react-swipeable-views": "^0.13.0",
		"@types/react-vertical-timeline-component": "^3.0.0",
		"@typescript-eslint/eslint-plugin": "^4.13.0",
		"@typescript-eslint/parser": "^4.13.0",
		"check-peer-deps": "^1.1.3",
		"del-cli": "^3.0.1",
		"eslint": "^7.11.0",
		"eslint-config-airbnb": "^18.2.1",
		"eslint-config-prettier": "^7.1.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jsdoc": "^31.0.5",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-prettier": "^3.3.1",
		"eslint-plugin-react": "^7.22.0",
		"eslint-plugin-react-hooks": "^4.2.0",
		"eslint-plugin-tsdoc": "^0.2.10",
		"eslint-plugin-yaml": "^0.3.0",
		"http-proxy-middleware": "^1.0.6",
		"netlify-lambda": "^2.0.2",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.2.1",
		"sort-package-json": "^1.48.1",
		"typescript": "^3.2.1"
	}
}
