{
	"name": "datapad",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"build": "run-p build:*",
		"build-clean": "run-s clean build",
		"build:backend": "netlify-lambda build -c ./webpack.backend.js ./backend",
		"build:frontend": "react-scripts build",
		"check-peer-dependencies": "check-peer-deps",
		"clean": "del-cli build/* backend-build/*",
		"eject": "react-scripts eject",
		"format": "run-s format:*",
		"format:ecmascript": "eslint ./{backend,src}/**/*.{js,jsx,ts,tsx} --fix",
		"format:package-json": "sort-package-json ./package.json",
		"format:prettier": "prettier . --write",
		"postinstall": "npm run check-peer-dependencies",
		"lint": "run-p lint:*",
		"lint:ecmascript": "eslint ./{backend,src}/**/*.{js,jsx,ts,tsx}",
		"lint:package-json": "sort-package-json ./package.json --check",
		"lint:prettier": "prettier . --check",
		"lint:yaml": "eslint .github/workflows/**/*.{yml,yaml}",
		"start": "run-p start:*",
		"start:backend": "netlify-lambda serve -c ./webpack.backend.js ./backend",
		"start:frontend": "react-scripts start",
		"test": "react-scripts test"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"@babel/runtime": "^7.11.2",
		"@material-ui/core": "^4.11.0",
		"@material-ui/icons": "^4.9.1",
		"@material-ui/lab": "^4.0.0-alpha.56",
		"@reduxjs/toolkit": "^1.3.6",
		"@widgetbot/react-embed": "^1.2.2",
		"aws-lambda": "^1.0.6",
		"bootstrap": "^4.5.0",
		"fs-extra": "^9.0.1",
		"jquery": "^3.5.1",
		"mongodb": "^3.4.0",
		"mongodb-client-encryption": "^1.1.0",
		"mongoose": "^5.9.23",
		"popper.js": "^1.16.1",
		"react": "^16.13.1",
		"react-animated-burgers": "^1.2.7",
		"react-custom-scrollbars": "^4.2.1",
		"react-dom": "^16.13.1",
		"react-iframe": "1.8.0",
		"react-image": "^4.0.3",
		"react-list": "^0.8.15",
		"react-markdown": "^4.3.1",
		"react-redux": "^7.2.0",
		"react-scripts": "^3.4.3",
		"react-swipeable-views": "^0.13.9",
		"redux": "^4.0.5",
		"styled-components": "^4.1.3",
		"typescript": "^3.9.5"
	},
	"devDependencies": {
		"@babel/core": "^7.10.4",
		"@babel/plugin-transform-object-assign": "^7.0.0",
		"@babel/preset-typescript": "^7.1.0",
		"@testing-library/dom": "^7.20.0",
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.5.0",
		"@testing-library/user-event": "^7.2.1",
		"@types/aws-lambda": "^8.10.59",
		"@types/jest": "^24.9.1",
		"@types/mongoose": "^5.7.31",
		"@types/node": "^12.12.44",
		"@types/react": "^16.9.35",
		"@types/react-burger-menu": "^2.6.1",
		"@types/react-custom-scrollbars": "^4.0.6",
		"@types/react-dom": "^16.9.8",
		"@types/react-list": "^0.8.5",
		"@types/react-redux": "^7.1.9",
		"@types/react-swipeable-views": "^0.13.0",
		"@types/react-vertical-timeline-component": "^3.0.0",
		"@typescript-eslint/eslint-plugin": "^2.34.0",
		"@typescript-eslint/parser": "^2.34.0",
		"check-peer-deps": "^1.1.3",
		"del-cli": "^3.0.1",
		"eslint": "^6.8.0",
		"eslint-config-airbnb": "^18.2.0",
