{
	"name": "datapad",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"build": "run-p build:*",
		"build:back-end": "netlify-lambda build ./backend",
		"build:front-end": "react-scripts build",
		"build-clean": "run-s clean build",
		"check-peer-dependencies": "check-peer-deps",
		"clean": "del-cli build/*",
		"eject": "react-scripts eject",
		"format": "run-s format:*",
		"format:ecmascript": "eslint ./src/**/*.{js,jsx,ts,tsx} --fix",
		"format:package-json": "sort-package-json ./package.json ./src/**/package.json",
		"format:prettier": "prettier . --write",
		"postinstall": "npm run check-peer-dependencies",
		"lint": "run-p lint:* --parallel",
		"lint:ecmascript": "eslint ./src/**/*.{js,jsx,ts,tsx}",
		"lint:package-json": "sort-package-json ./package.json ./src/**/package.json --check",
		"lint:prettier": "prettier . --check",
		"start": "run-p start:*",
		"start:back-end": "netlify-lambda serve ./backend",
		"start:front-end": "react-scripts start",
		"test": "react-scripts test"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"@reduxjs/toolkit": "^1.3.6",
		"@widgetbot/react-embed": "^1.2.2",
		"bootstrap": "^4.5.0",
		"jquery": "^3.5.1",
		"popper.js": "^1.16.0",
		"react": "^16.13.1",
		"react-animated-burgers": "^1.2.7",
		"react-bootstrap": "^1.0.1",
		"react-burger-menu": "^2.6.15",
		"react-dom": "^16.13.1",
		"react-iframe": "1.8.0",
		"react-list": "^0.8.15",
		"react-redux": "^7.2.0",
		"react-scripts": "^3.4.1",
		"react-vertical-timeline-component": "^3.0.2",
		"redux": "^4.0.5",
		"styled-components": "^4.1.3",
		"typescript": "^3.9.5"
	},
	"devDependencies": {
		"@testing-library/dom": "^7.20.0",
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.5.0",
		"@testing-library/user-event": "^7.2.1",
		"@types/jest": "^24.9.1",
		"@types/node": "^12.12.44",
		"@types/react": "^16.9.35",
		"@types/react-burger-menu": "^2.6.1",
		"@types/react-dom": "^16.9.8",
		"@types/react-list": "^0.8.5",
		"@types/react-redux": "^7.1.9",
		"@types/react-vertical-timeline-component": "^3.0.0",
		"@typescript-eslint/eslint-plugin": "^2.34.0",
		"@typescript-eslint/parser": "^2.34.0",
		"check-peer-deps": "^1.1.3",
		"del-cli": "^3.0.1",
		"eslint": "^6.8.0",
		"eslint-config-airbnb": "^18.1.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-jsdoc": "^20.4.0",
		"eslint-plugin-jsx-a11y": "^6.3.1",
		"eslint-plugin-prettier": "^3.1.3",
		"eslint-plugin-react": "^7.20.0",
		"eslint-plugin-react-hooks": "^4.0.7",
		"netlify-lambda": "^1.6.3",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.0.0",
		"prettier-plugin-organize-imports": "^1.1.0",
		"sort-package-json": "^1.44.0"
	}
}
